   "source": [
    "uploaded = files.upload()\n",
    "\n",
    "if len(uploaded) != 1:\n",
    "  print(\"Upload exactly 1 file for source.\")\n",
    "else:\n",
    "  for k, v in uploaded.items():\n",
    "    _, ext = os.path.splitext(k)\n",
    "    os.remove(k)\n",
    "    SOURCE_NAME = f\"source{ext}\"\n",
    "    open(SOURCE_NAME, 'wb').write(v)"
   ]
